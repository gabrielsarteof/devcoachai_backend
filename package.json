  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src --format cjs,esm --dts --clean --tsconfig tsconfig.build.json",
    "start": "node dist/server.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:tests": "tsc -p tsconfig.spec.json --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:auth": "vitest tests/modules/auth",
    "test:challenges": "vitest tests/modules/challenges",
    "test:metrics": "vitest tests/modules/metrics",
    "test:ai": "vitest --config vitest.ai.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:setup": "docker-compose -f docker-compose.test.yml up -d && timeout /t 10 && npm run db:push:test",
    "test:teardown": "docker-compose -f docker-compose.test.yml down -v",
    "test:reset": "npm run test:teardown && npm run test:setup",
    "db:push:test": "cross-env DATABASE_URL=\"postgresql://postgres:postgres@localhost:5433/devai_coach_test\" prisma db push",
    "db:reset:test": "cross-env DATABASE_URL=\"postgresql://postgres:postgres@localhost:5433/devai_coach_test\" prisma migrate reset --force",
    "test:full": "npm run test:setup && npm run test && npm run test:teardown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write '**/*.{ts,js,json,md}'",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "prepare": "husky install"
  },
